+  }
+
+#define HEAP_INIT(header) (header)->head = NULL;
+
+#define HEAP_EMPTY(header) (((header)->head == NULL) ? 1 : 0)
+
+#define HEAP_HEAD(header) ((header)->head)
+
+#define HEAP_NEXT(entry, field) ((entry)->field.next)
