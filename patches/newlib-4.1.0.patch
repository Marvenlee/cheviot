+    if (_nsyydebug)
+    {
+        fprintf(stderr, "%s[%d]: after reduction, ", _NSYYDEBUGSTR, _nsyydepth);
+#ifdef _NSYYSTYPE_TOSTRING
+#if _NSYYBTYACC
+        if (!_nsyytrial)
