+	char		*buffer	= va_arg(ap, char *);
+	size_t		 buflen	= va_arg(ap, size_t);
+	struct group   **result	= va_arg(ap, struct group **);
+
+	struct __grstate_nis state;
+	int	rv;
+
+	_DIAGASSERT(retval != NULL);
