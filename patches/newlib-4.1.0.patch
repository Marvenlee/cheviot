+int
+__grstart_files(struct __grstate_files *state)
+{
+
+	_DIAGASSERT(state != NULL);
+
+	if (state->fp == NULL) {
+		state->fp = fopen(_PATH_GROUP, "re");
+		if (state->fp == NULL)
+			return NS_UNAVAIL;
+	} else {
+		rewind(state->fp);
+	}
+	return NS_SUCCESS;
+}
