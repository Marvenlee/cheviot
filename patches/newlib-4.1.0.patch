-#else
-  register int r0 asm("r0") = fh;
-  register int r1 asm("r1") = (int) ptr;
-  register int r2 asm("r2") = (int) len;
-  asm ("swi %a4"
-       : "=r" (r0)
-       : "0"(r0), "r"(r1), "r"(r2), "i"(SWI_Read));
-  return r0;
-#endif
