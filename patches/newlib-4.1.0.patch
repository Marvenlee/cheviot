+	int	  bywhat = va_arg(ap, int);
+
+	DBT	 key, data;
+	size_t	 len;
+	char	*ks;
+	int	 r;
+
+	if (_ng_db == NULL)
+		return NS_UNAVAIL;
+
+	len = strlen(name) + 2;
+	ks = malloc(len);
+	if (ks == NULL)
+		return NS_UNAVAIL;
+
+	ks[0] = bywhat;
+	(void)memcpy(&ks[1], name, len - 1);
+
+	key.data = (u_char *)ks;
+	key.size = len;
+
+	r = (*_ng_db->get)(_ng_db, &key, &data, 0);
+	free(ks);
+	switch (r) {
+	case 0:
+		break;
+	case 1:
+		return NS_NOTFOUND;
+	case -1:
+			/* XXX: call endnetgrent() here ? */
+		return NS_UNAVAIL;
+	}
+
+	*line = strdup(data.data);
+	if (*line == NULL)
+		return NS_UNAVAIL;
+	return NS_SUCCESS;
+}
