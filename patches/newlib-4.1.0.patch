+struct lastlogx {
+	struct timeval ll_tv;		/* time entry was created */
+	char ll_line[_UTX_LINESIZE];	/* tty name */
+	char ll_host[_UTX_HOSTSIZE];	/* host name */
+#if 0 
+	struct sockaddr_storage ll_ss;	/* address where entry was made from */
+#endif
+};
+#endif	/* !_XOPEN_SOURCE */
+
+__BEGIN_DECLS
+
