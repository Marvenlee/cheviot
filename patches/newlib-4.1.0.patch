+#define	_IN_PORT_T_DECLARED
+#endif
+
+typedef	__uintptr_t	u_register_t;
+#endif /* __BSD_VISIBLE */
+
+#define NBBY	8									/* Number of Bits per Byte */
+
+#if __MISC_VISIBLE
+#ifndef _BSDTYPES_DEFINED
