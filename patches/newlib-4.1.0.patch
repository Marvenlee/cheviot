+#define LOGIN_PROCESS	6
+#define USER_PROCESS	7
+#define DEAD_PROCESS	8
+
+#if defined(_NETBSD_SOURCE)
+#define ACCOUNTING	9
+#define SIGNATURE	10
+#define DOWN_TIME	11
