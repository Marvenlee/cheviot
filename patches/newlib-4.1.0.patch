+/*
+ *
+ */
+int isatty(int fd)
+{   
+    int sc;
+    sc = _swi_isatty(fd);  
+
+    if (sc < 0)
+    {
+        errno = -sc;
+        return 0;
+    }
+    
+    return sc;
+}
