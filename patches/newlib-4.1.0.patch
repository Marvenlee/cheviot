+	_DIAGASSERT(pp != NULL);
+	_DIAGASSERT(*pp != NULL);
+
+	ng = malloc(sizeof(struct netgroup));
+	if (ng == NULL)
+		return NULL;
+
+	(*pp)++;	/* skip '(' */
+	if (!getstring(pp, ',', &ng->ng_host))
+		goto badhost;
+
+	if (!getstring(pp, ',', &ng->ng_user))
+		goto baduser;
+
+	if (!getstring(pp, ')', &ng->ng_domain))
+		goto baddomain;
+
+#ifdef DEBUG_NG
+	{
+		char buf[1024];
+		(void) fprintf(stderr, "netgroup %s\n",
+		    _ng_print(buf, sizeof(buf), ng));
+	}
+#endif
+	return ng;
+
+baddomain:
+	if (ng->ng_user)
+		free(ng->ng_user);
+baduser:
+	if (ng->ng_host)
+		free(ng->ng_host);
+badhost:
+	free(ng);
+	return NULL;
+}
