+
+#define HEAP_INITIALIZER                                                       \
+  { NULL }
+
+#define HEAP_TYPE(type)                                                        \
+  typedef struct { struct type *head; }
+
+#define HEAP(type)                                                             \
+  struct {                                                                     \
+    struct type *head;                                                         \
+  }
+
+#define HEAP_DECLARE(name, type)                                               \
+  struct name {                                                                \
+    struct type *head;                                                         \
+  }
+
+#define HEAP_DEFINE(name) struct name
+
+#define HEAP_ENTRY(type)                                                       \
+  struct {                                                                     \
+    struct type *next;                                                         \
