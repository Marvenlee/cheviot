+int
+__grend_files(struct __grstate_files *state)
+{
+
+	_DIAGASSERT(state != NULL);
+
+	if (state->fp) {
+		(void) fclose(state->fp);
+		state->fp = NULL;
+	}
+	return NS_SUCCESS;
+}
