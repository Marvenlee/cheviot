+            _nsyyn <= _NSYYTABLESIZE && _nsyycheck[_nsyyn] == (_NSYYINT) _nsyystate)
+        _nsyystate = _nsyytable[_nsyyn];
+    else
+        _nsyystate = _nsyydgoto[_nsyym];
