+static struct passwd		_files_passwd;
+static char			_files_passwdbuf[_GETPW_R_SIZE_MAX];
+
+static int
+_files_start(struct files_state *state)
+{
+	int	rv;
+
+	_DIAGASSERT(state != NULL);
+
+	state->keynum = 0;
+	rv = _pw_opendb(&state->db, &state->version);
