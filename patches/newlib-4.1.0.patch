+#include <sys/cdefs.h>
+__RCSID("$NetBSD: reallocarray.c,v 1.5 2015/07/26 02:22:33 kamil Exp $");
+
+#define _OPENBSD_SOURCE
+#include <errno.h>
+#include <stdlib.h>
+
+void *
+reallocarray(void *optr, size_t nmemb, size_t size)
+{
+	if (nmemb == 0 || size == 0)
+		return realloc(optr, 0);
+
+	int e = reallocarr(&optr, nmemb, size);
+
+	if (e == 0)
+		return optr;
+	errno = e;
+	return NULL;
+}
